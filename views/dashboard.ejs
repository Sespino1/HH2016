<!DOCTYPE html>
<html>
  <head>
    <%- include ../partials/head.ejs %>
    <%- include ../partials/scripts.ejs %>
  </head>
  <body>
    <div class="row">
      <div class="small-10 small-centered columns">
        <h1><%= title %></h1>
        <div id="welcomeMessage">
          Welcome, <span id="customerName">  </span>
        </div>
      </div>
    </div>


    <div class= "row">
      <div  class="small-10 small-centered columns">
        <span id="cardNumbers"> </span>
      <% for (var i = 0; i < cards.length; i++) { %>
        <div>
          <span><%= cards[i][0] %></span><br>
          <span> <h1><%= "$"+ cards[i][1] %> </h1> </span><br>
          <span> Rewards </span><br><hr />
        </div>
      <% } %>
      </div>
    </div>
    <br>


  </body>
</html>
