<!DOCTYPE html>
<html>
  <head>
    <%- include ../partials/head.ejs %>
  </head>
  <body>
    <div class="row">
      <div class="small-10 small-centered columns">
        <h1><%= title %></h1>
      </div>
    </div>

    <div class= "row">
      <div  class="small-10 small-centered columns">
      <% for (var i = 0; i < cards.length; i++) { %>
        <div>
          <span><%= cards[i][0] %></span><br>
          <span> <h1><%= "$"+ cards[i][1] %> </h1> </span><br>
          <span> Rewards </span><br><hr />
        </div>
      <% } %>
      </div>
    </div>


  </body>
</html>
