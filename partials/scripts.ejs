<script data-main="lib/capital_one" src="lib/require-jquery.js"></script>
<script>
  $(function(){
    require(['customer'], function (customer) {
      var apikey = 'c24b7c966f60b4c4200a10bbfe8c3b69';
      customerDemo(apikey, customer);
    });
  });
function customerDemo (apikey, customer) {
  var customerAccount = customer.initWithKey(apikey);
  var customerInfo = customerAccount.getCustomers()[0];

  console.log("Sample Customer: " + customerInfo.first_name);
  $('#customerName').html(customerInfo.first_name + " " + customerInfo.last_name );

}

$(function(){
  require(['account'], function (account) {
    var apikey = 'c24b7c966f60b4c4200a10bbfe8c3b69';
    accountDemo(apikey, account);
  });
});

function accountDemo (apikey, account, cust_id) {
  var customerAccount = account.initWithKey(apikey);
  var accountInfo = customerAccount.getAllAccounts()[0];
  console.log(accountInfo.account_number);
  $('#cardNumbers').html(accountInfo.account_number );
}

function openMenu(id) {
  $('#' + id).toggleClass('open');
}

function openOverlay(id) {
  $('#' + id).addClass('open');
}

function closeOverlay(id) {
  $('#' + id).removeClass('open');
}
</script>
